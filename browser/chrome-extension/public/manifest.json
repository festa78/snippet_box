{
  "manifest_version": 2,
  "name": "Chrome Extension to save URL to database",
  "description": "Chrome Extension to save URL to database",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/vendor.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}